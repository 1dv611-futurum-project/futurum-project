{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "ter-indent": [true, 2],
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "ordered-imports": false,
        "no-console": false,
        "member-ordering": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}