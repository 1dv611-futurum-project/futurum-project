    "@types/socket.io": "^1.4.33",
    "@types/websocket": "0.0.38",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "googleapis": "^28.1.0",
    "http": "0.0.0",
    "imap": "^0.8.19",
    "jsonwebtoken": "^8.2.1",
    "mailparser-mit": "^0.6.2",
    "mongoose": "^5.0.15",
    "nodemon": "^1.17.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
