    "mocha": "^5.1.1",
    "sinon": "^4.5.0",
    "source-map-loader": "^0.2.3",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^2.8.1"
  }
