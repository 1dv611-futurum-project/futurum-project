{
  "name": "server-boilerplate",
  "version": "1.0.0",
  "description": "An Express/Node.js boilerplate for the server.",
  "main": "app.js",
  "scripts": {
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- -r dotenv/config ./src/index.ts",
    "test": "mocha -r ts-node/register test/**/*.test.ts"
  },
  "keywords": [
    "node",
    "express",
    "boilerplate",
    "server"
  ],
